<h1>Sign In</h1>
<%= form_tag('/sessions', method: "post") do %>
    <div>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </div>
    <div>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div>
      <%= submit_tag "Sign In" %>
    </div>
<% end %>